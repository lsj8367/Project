<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>

		<!-- 사용자 -->
		<typeAlias type="pack.user.model.UserDto" alias="userdto" />

		<!-- 새 도서 -->
		<typeAlias type="pack.newbook.model.NewBookDto" alias="nbdto" />

		<!-- 중고도서 -->
		<typeAlias type="pack.oldbook.model.OldBookDto" alias="obdto" />

		<!-- 카드정보 -->
		<typeAlias type="pack.cardinfo.model.CardInfoDto" alias="carddto" />

		<!-- 후기 -->
		<typeAlias type="pack.review.model.ReviewDto" alias="reviewdto" />

		<!-- 주문내역 -->
		<typeAlias type="pack.orderinfo.model.OrderInfoDto" alias="orderdto" />

		<!-- FAQ -->
		<typeAlias type="pack.faqboard.model.FaqBoardDto" alias="faqdto" />

		<!-- 1:1 문의 -->
		<typeAlias type="pack.inquery.model.InqueryDto" alias="inqdto" />

		<!-- 대여 정보 -->
		<typeAlias type="pack.rentinfo.model.RentInfoDto" alias="rdto" />

	</typeAliases>
</configuration>
